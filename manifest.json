{
	"name": "My Extension",
	"description": "Enterkey Test Extension",
	"version": "1.0",
	"permissions": ["contextMenus", "tabs", "http://*/*", "https://*/*", "<all_urls>"],
	"background": {
		"page": "/resources/javascript/background.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": ["/resources/stylesheets/common.css"],
			"js": ["/resources/javascript/main.js", "/resources/lib/jquery/jquery-1.9.1.js"],
			"run_at": "document_end",
			"all_frames": true
		}
	],
	"web_accessible_resources": [
		"chrome_ex_oauth.html",
		"chrome_ex_oauthsimple.js",
		"chrome_ex_oauth.js",
		"onload.js"		
	],
	"manifest_version": 2
}
