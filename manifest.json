{
	"name": "My Extension",
	"description": "Enterkey Test Extension",
	"version": "1.0",
	"permissions": ["contextMenus", "tabs", "https://accounts.google.com/o/oauth2/token", "https://www.googleapis.com/", "http://*/*", "https://*/*", "<all_urls>"],
	"background": {
		"page": "/resources/javascript/background.html"
	},
	"content_scripts": [
		{
			"matches": ["http://www.google.com/robots.txt*"],
			"js": ["/oauth2/oauth2_inject.js"],
			"run_at": "document_start"
		},
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": ["/resources/stylesheets/common.css"],
			"js": ["/resources/javascript/main.js", "/resources/lib/jquery/jquery-1.9.1.js"],
			"run_at": "document_end",
			"all_frames": true
		}
	],
	"web_accessible_resources": [
		"oauth2/oauth2.html"
	],
	"manifest_version": 2
}
